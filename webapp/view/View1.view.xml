<mvc:View controllerName="app.split.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
    xmlns="sap.m">
   	<Page id="page" title="{}i18n>title}">
		<headerContent>
			<Button icon="sap-icon://sort" press="onSort"></Button>
			<Button icon="sap-icon://action" tooltip="Share" press="onPressToView2"/>
		</headerContent>
		<subHeader>
			<OverflowToolbar>
				<SearchField search="onSearch" id="idSearch" liveChange="onSearch"/>
			</OverflowToolbar>
		</subHeader>
		<content>
			<List items="{toolModel>/toolsData}" id="idListItem" mode="SingleSelectMaster"
			selectionChange="onItemPress">
				<items>
					<ObjectListItem icon="{toolModel>icon}" intro="{toolModel>id}" number="{toolModel>price}" 
					numberState="Success"
						numberUnit="{toolModel>currency}" title="{toolModel>name}">
						<attributes>
							<ObjectAttribute title="weight" text="{toolModel>weight}"></ObjectAttribute>
						</attributes>
						<firstStatus>
							<ObjectStatus title="status" text="{toolModel>status}" state="Success"></ObjectStatus>
						</firstStatus>
					</ObjectListItem>
					<!--<DisplayListItem  label="name" value="mitre saw" > </DisplayListItem>-->
					<!--<InputListItem label="name">-->
					<!--	<content>-->
					<!--		<Label text="description"></Label>-->
					<!--		<Input value="good for cutting "></Input>-->
					<!--	</content>-->
					<!--</InputListItem>-->
					<!--<StandardListItem icon="data:image/jpeg;base64," -->
					<!--description="for smooth cuts" info="600" title="mitre saw"></StandardListItem>-->
					<!--<FeedListItem icon="data:image/jpeg;base64,"-->
					<!--    info="sonya" -->
					<!--    sender="Surya" text="thanks for liking the film"></FeedListItem>-->
				</items>
			</List>
		</content>
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				
				<Button text="Create" type="Accept" press="onPressForm"/>
			
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>